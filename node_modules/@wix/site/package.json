{
  "name": "@wix/site",
  "version": "1.21.0",
  "license": "UNLICENSED",
  "description": "The Site API enables code within Embedded Scripts and Site Widgets created with the CLI to interact with a Wix site. For example, you can access site data and interact with other Wix Apps, such as Wix Stores and Wix Bookings.",
  "keywords": [
    "wix sdk",
    "@wix/sdk",
    "wix site",
    "wix website"
  ],
  "homepage": "https://dev.wix.com/docs/sdk/host-modules/site/introduction",
  "author": {
    "name": "Emilis Baliukonis",
    "email": "emilisb@wix.com"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types//index.d.ts"
    },
    "./testkit": {
      "import": "./dist/esm/testkit/index.js",
      "require": "./dist/cjs/testkit/index.js",
      "types": "./dist/types/testkit/index.d.ts"
    }
  },
  "sideEffects": false,
  "types": "dist/types/index.d.ts",
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "publishScoped": true,
  "scripts": {
    "build": "yoshi-library build",
    "start": "yoshi-library start",
    "test": "yoshi-library test",
    "lint": "yoshi-library lint --fix"
  },
  "lint-staged": {
    "*.{js,ts}": "yarn lint"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0",
    "@wix/sdk-types": "^1.13.2"
  },
  "devDependencies": {
    "@types/chance": "^1.1.6",
    "@types/jest": "^27.4.0",
    "@types/node": "^16.0.0",
    "@wix/eslint-config-yoshi": "^6.0.0",
    "@wix/essentials": "^0.1.14",
    "@wix/jest-yoshi-preset": "^6.0.0",
    "@wix/sdk": "^1.15.9",
    "@wix/yoshi-flow-library": "^6.0.0",
    "chance": "^1.1.12",
    "ts-jest": "^27.1.2",
    "typescript": "^5.2.2"
  },
  "jest": {
    "preset": "@wix/jest-yoshi-preset"
  },
  "yoshiFlowLibrary": {
    "bundle": true,
    "buildEsmWithBabel": true
  },
  "eslintConfig": {
    "extends": "@wix/eslint-config-yoshi"
  },
  "wix": {
    "artifact": {
      "groupId": "com.wixpress",
      "artifactId": "website-sdk"
    },
    "validations": {
      "postDependenciesBuild": [
        "lint"
      ]
    }
  },
  "wallaby": {
    "autoDetect": true
  },
  "falconPackageHash": "f3ed2c716e21221fdcd28daad77cc368386994970aca029ee08d071a"
}